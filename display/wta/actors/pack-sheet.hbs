<form class="{{cssClass}} flexcol{{#if locked}} locked{{/if}}" autocomplete="off">
    {{!-- Pack Sheet Header --}}
    <header class="actor-header" {{#if headerbg}} style="background: url({{headerbg}}) center;"{{/if}}>
        {{> "systems/vtm5e/display/shared/actors/parts/group/header-profile.hbs"}}
    </header>

    {{!-- Tab Navigation --}}
    {{> "systems/vtm5e/display/shared/actors/parts/group/navbar.hbs"}}

    {{!-- Coterie Sheet Body --}}
    <section class="sheet-body">
        {{!-- Group Members Tab --}}
        {{> "systems/vtm5e/display/shared/actors/parts/group/group-members.hbs"}}

        {{!-- Features Tab --}}
        {{> "systems/vtm5e/display/shared/actors/parts/features.hbs"}}

        {{!-- Notes Tab --}}
        {{> "systems/vtm5e/display/shared/actors/parts/notepad.hbs"}}

        {{!-- Settings Tab --}}
        {{> "systems/vtm5e/display/shared/actors/parts/actor-settings.hbs"}}
    </section>
</form>
